.container {
    width: 100%;
    height: 100%;
    position: relative;
    top: 0;
    background-color: #fff;
    left: 0;
}

.birthday_img {
    width: 131rpx;
    height: 170rpx;
    padding: 36rpx 0;
}

.birth_name_sex {
    width: 100%;
    padding: 0 30rpx;
    box-sizing: border-box;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
}

.name {
    font-size: 30rpx;
    color: #545353;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.sex {
    font-size: 30rpx;
    border-left: 1px solid #d5d5d5;
    color: #545353;
    padding-left: 80rpx;
    margin-left: 30rpx;
}

.woman {
    background: url(data:image/jpeg;base64,) no-repeat 30rpx center;
    background-size: 20rpx 28rpx;
}

.man {
    background: url(data:image/jpeg;base64,);
    background-repeat: no-repeat;
    background-position: 30rpx center;
    background-size: 27rpx 27rpx;
}

.birthday_txt {
    font-size: 30rpx;
    padding: 30rpx 0;
    border-bottom: 1rpx solid #d5d5d5;
    width: 100%;
    text-align: center;
}

.highlight {
    font-size: 42px;
    color: #f8913e;
    margin: 0 10px;
}

.note_time {
    width: 100%;
    line-height: 60px;
    padding-left: 50px;
    box-sizing: border-box;
    background: url(data:image/png;base64,) no-repeat 30rpx 42rpx;
    background-size: 34rpx 34rpx;
    border-bottom: 1rpx solid #d5d5d5;
    font-size: 30rpx;
}

.birthday_time {
    width: 100%;
    line-height: 60px;
    padding-left: 50px;
    box-sizing: border-box;
    background: url(data:image/jpeg;base64,) no-repeat 30rpx center;
    background-size: 40rpx 35rpx;
    border-bottom: 1rpx solid #d5d5d5;
    font-size: 30rpx;
}

.alarm-text {
    font-size: 30rpx;
    color: #888e92;
    margin-bottom: 36rpx;
    width: 34%;
    float: left;
    margin-right: 80rpx;
}

.birthday_remind {
    width: 100%;
    border-bottom: 1rpx solid #d5d5d5;
    background: url(data:image/jpeg;base64,) no-repeat 30rpx 48rpx;
    background-size: 38rpx 34rpx;
    padding-left: 100rpx;
    box-sizing: border-box;
}

.note_remind {
    width: 100%;
    border-bottom: 1rpx solid #d5d5d5;
    background: url(data:image/png;base64,) no-repeat 30rpx 48rpx;
    background-size: 40rpx 40rpx;
    padding-left: 100rpx;
    box-sizing: border-box;
}

.note_img {
    width: 128rpx;
    height: 128rpx;
    padding: 36rpx 0;
}

.to-index {
    color: #35adec;
    margin-top: 80rpx;
    font-size: 30rpx;
    padding-bottom: 80rpx;
}